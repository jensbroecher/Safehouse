<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.sfh.music"
        versionCode = "6" 
        version     = "6.0.0" >

    <name>SAFEHOUSE RECORDS</name>

    <description>
		SafeHouse Records is a multinational, multi exceptional music production stable.
    </description>

    <author href="https://www.jthglobal.com" email="info@jthglobal.com">
        JTH Consulting Kenya Ltd.
    </author>
	
	<icon src="logo.png" />
		
	<preference name="android-minSdkVersion" value="9" /> 
	<preference name="android-targetSdkVersion" value="19" />
	
